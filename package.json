    "@primevue/core": "catalog:",
    "@primeuix/themes": "catalog:",
    "@ag/agent": "workspace:*",
    "@ag/ui": "workspace:*",
    "@ag/form-validation": "workspace:*",
    "@ag/schemas": "workspace:*",
    "@ag/test-utils": "workspace:*",
    "@ag/admin-message": "workspace:*",
    "@ag/assets": "workspace:*",
    "qalendar": "catalog:",
    "quill": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "vue-tsc": "catalog:",
    "vite": "catalog:",
