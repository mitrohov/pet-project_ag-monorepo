{
  "name": "ag-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "mitrohov-dmitriy",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "up": "pnpm -r i && pnpm delete-builds",
    "up-deep": "pnpm delete-all-deps && pnpm delete-builds && pnpm -r i",
    "dev-crm": "pnpm --filter @ag/crm dev",
    "build-crm": "pnpm --filter @ag/crm build",
    "dev-api": "pnpm --filter @ag/api dev",
    "build-api": "pnpm --filter @ag/api build",
    "start-api": "pnpm --filter @ag/api start:prod",
    "studio": "pnpm --filter @ag/api studio",
    "migrate": "pnpm --filter @ag/api migrate",
    "generate-client": "pnpm --filter @ag/api generate-client",
    "dev-docs": "pnpm --filter @ag/docs dev",
    "build-docs": "pnpm --filter @ag/docs build",
    "dev-landing": "pnpm --filter @ag/landing dev",
    "build-landing": "pnpm --filter @ag/landing build",
    "prettier": "prettier --write '**/*.{js,ts,jsx,tsx,cjs,cts,mjs,mts,vue,astro}'",
    "lint": "pnpm prettier && pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "delete-all-deps": "rm -rf pnpm-lock.yaml && pnpm -r delete-deps && pnpm cache delete",
    "delete-builds": "pnpm -r delete-build",
    "commit": "pnpm typecheck && pnpm lint && git add . && git commit -m 'save changes' && git push"
  },
  "dependencies": {
    "pinia": "catalog:",
    "vue-router": "catalog:",
    "primeicons": "catalog:",
    "primevue": "catalog:",
    "@primeuix/themes": "catalog:"
  },
  "devDependencies": {
    "vitest": "catalog:",
    "vitest-browser-vue": "catalog:",
    "flatpickr": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "@ag/eslint": "workspace:*",
    "@ag/tsconfig": "workspace:*"
  },
  "keywords": [],
  "packageManager": "pnpm@10.11.0"
}
